{
  "name": "winky-coder",
  "version": "2.0.0",
  "description": "The Ultimate Full-Stack AI IDE - Beat Cursor, VS Code, and Firebase Studio",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "format": "prettier --write .",
    "setup": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "docker:build": "docker build -t winky-coder .",
    "docker:run": "docker run -p 3000:3000 -p 3001:3001 winky-coder",
    "deploy:vercel": "vercel --prod",
    "deploy:railway": "railway up"
  },
  "keywords": [
    "ai",
    "ide",
    "full-stack",
    "development",
    "cursor",
    "vscode",
    "firebase",
    "react",
    "nextjs",
    "nodejs",
    "typescript",
    "monaco-editor",
    "terminal",
    "git",
    "deployment"
  ],
  "author": "Winky-Coder Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/winky-coder/winky-coder.git"
  },
  "bugs": {
    "url": "https://github.com/winky-coder/winky-coder/issues"
  },
  "homepage": "https://winky-coder.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "workspaces": [
    "backend",
    "frontend",
    "shared"
  ]
}